{
  "manifest_version": 3,
  "name": "Sunbird Editor",
  "version": "1.0",
  "description": "Editor to translate text into many indian languages by leveraging the power of AI",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon-16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon-16.png"
  }
}
